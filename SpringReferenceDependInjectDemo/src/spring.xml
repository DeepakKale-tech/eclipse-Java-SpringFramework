<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- bean definitions here -->

	<bean id="a1" class="com.model.Address">
		<property name="city" value="Shangai" />
		<property name="state" value="Tokyo" />
		<property name="country" value="Japan" />
		<property name="pincode" value="123390" />
	</bean>

	<bean id="emp1" class="com.model.Employee">
		<property name="id" value="101" />
		<property name="name" value="Ching Chong" />
		<property name="address" ref="a1" />
	</bean>
</beans>